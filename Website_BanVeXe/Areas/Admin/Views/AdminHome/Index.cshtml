<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Required meta tags-->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Title Page-->
    <title>Home</title>
    <!-- Fontfaces CSS-->
    <script src=" https://code.jquery.com/jquery-3.5.1.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <link href="https://fonts.googleapis.com/css?family=Rubik:300,400,700|Oswald:400,700" rel="stylesheet">
    <link href="~/Areas/Admin/Themes/css/font-face.css" rel="stylesheet" media="all">
    <link href="~/Areas/Admin/Themes/vendor/font-awesome-4.7/css/font-awesome.min.css" rel="stylesheet" media="all">
    <link href="~/Areas/Admin/Themes/vendor/font-awesome-5/css/fontawesome-all.min.css" rel="stylesheet" media="all">
    <link href="~/Areas/Admin/Themes/vendor/mdi-font/css/material-design-iconic-font.min.css" rel="stylesheet" media="all">
    <!-- Bootstrap CSS-->
    <link href="~/Areas/Admin/Themes/vendor/bootstrap-4.1/bootstrap.min.css" rel="stylesheet" media="all">
    <!-- Vendor CSS-->
    <link href="~/Areas/Admin/Themes/vendor/animsition/animsition.min.css" rel="stylesheet" media="all">
    <link href="~/Areas/Admin/Themes/vendor/bootstrap-progressbar/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet" media="all">
    <link href="~/Areas/Admin/Themes/vendor/wow/animate.css" rel="stylesheet" media="all">
    <link href="~/Areas/Admin/Themes/vendor/css-hamburgers/hamburgers.min.css" rel="stylesheet" media="all">
    <link href="~/Areas/Admin/Themes/vendor/slick/slick.css" rel="stylesheet" media="all">
    <link href="~/Areas/Admin/Themes/vendor/select2/select2.min.css" rel="stylesheet" media="all">
    <link href="~/Areas/Admin/Themes/vendor/perfect-scrollbar/perfect-scrollbar.css" rel="stylesheet" media="all">
    <!-- Main CSS-->
    <link href="~/Areas/Admin/Themes/css/theme.css" rel="stylesheet" media="all">
</head>
<body class="animsition">
    <div class="page-wrapper">
        <!-- MENU SIDEBAR-->
        <aside class="menu-sidebar d-none d-lg-block">
            <div class="logo">
                <a href="#">
                    <h2 style="color:#ff8b00">PHUONGNGAN</h2>
                </a>
            </div>
            <div class="menu-sidebar__content js-scrollbar1">
                <nav class="navbar-sidebar">
                    <ul class="list-unstyled navbar__list">
                        <li class="active has-sub">
                            <a class="js-arrow" href="#">
                                <i class="fas fa-table"></i>Quản lý chung
                            </a>
                            <ul class="list-unstyled navbar__sub-list js-sub-list">
                                <li>
                                    <a href='@Url.Action("Index", "DSKhachHang")'>Danh sách khách hàng</a>
                                </li>
                                <li>
                                    <a href='@Url.Action("Index", "TaiVaPhuXe")'>Tài và phụ xe</a>
                                </li>
                                <li>
                                    <a href='@Url.Action("Index", "Xe")'>Xe lưu hành</a>
                                </li>
                                <li>
                                    <a href='@Url.Action("Index", "DiemDon")'>Điểm đón khách</a>
                                </li>
                                <li>
                                    <a href='@Url.Action("Index", "TuyenDi")'>Tuyến đường chạy</a>
                                </li>
                            </ul>
                        </li>
                        <li class="has-sub">
                            <a class="js-arrow" href="#">
                                <i class="fas fa-tachometer-alt"></i>Đặt chỗ/ Bán vé
                            </a>
                            <ul class="list-unstyled navbar__sub-list js-sub-list">
                               
                                <li>
                                    <a href='@Url.Action("Index", "ChuyenDi")'>Lịch khởi hành</a>
                                </li>
                                <li>
                                    <a href='@Url.Action("Index", "DatVeChoKhach")'>Đặt vé cho khách</a>
                                </li>
                              
                              
                            </ul>
                        </li>
                        <li>
                            <a href="chart.html">
                                <i class="fas fa-chart-bar"></i>Báo cáo, thống kê
                            </a>
                        </li>
                     
                      
                    </ul>
                </nav>
            </div>
        </aside>
        <!-- END MENU SIDEBAR-->
        <!-- PAGE CONTAINER-->
        <div class="page-container">
            <!-- HEADER DESKTOP-->
            <header class="header-desktop">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                        <div class="header-wrap">
                            <form class="form-header" action="" method="POST">
                                <input class="au-input au-input--xl" type="text" name="search" placeholder="Tìm kiếm" />
                                <button style="background-color:#ff8b00" class="au-btn--submit " type="submit">
                                    <i class="zmdi zmdi-search"></i>
                                </button>
                            </form>
                            <div class="header-button">
                                <div class="noti-wrap">
                                    <div class="noti__item js-item-menu">
                                        <i class="zmdi zmdi-comment-more"></i>
                                        <span class="quantity">1</span>
                                        <div class="mess-dropdown js-dropdown">
                                            <div class="mess__title">
                                                <p>Bạn có 1 tin nhắn mới</p>
                                            </div>
                                            <div class="mess__item">
                                                <div class="image img-cir img-40">
                                                    <img src="~/Areas/Admin/Themes/images/icon/avatar-06.jpg" alt="Michelle Moreno" />
                                                </div>
                                                <div class="content">
                                                    <h6>Tiên Ngân</h6>
                                                    <p>Đã gửi cho bạn một hình ảnh</p>
                                                    <span class="time">3 phút trước</span>
                                                </div>
                                            </div>
                                          
                                            <div class="mess__footer">
                                                <a href="#">Xem tất cả tin nhắn</a>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="noti__item js-item-menu">
                                        <i class="zmdi zmdi-email"></i>
                                        <span class="quantity">1</span>
                                        <div class="email-dropdown js-dropdown">
                                            <div class="email__title">
                                                <p>Bạn có 1 email mới</p>
                                            </div>
                                            <div class="email__item">
                                                <div class="image img-cir img-40">
                                                    <img src="~/Areas/Admin/Themes/images/icon/avatar-06.jpg" alt="Cynthia Harvey" />
                                                </div>
                                                <div class="content">
                                                    <p>Cuộc họp về team FrontEnd...</p>
                                                    <span>Sếp, 3 phút trước</span>
                                                </div>
                                            </div>
                                          
                                         
                                            <div class="email__footer">
                                                <a href="#">Xem tất cả các email</a>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="noti__item js-item-menu">
                                        <i class="zmdi zmdi-notifications"></i>
                                        <span class="quantity">3</span>
                                        <div class="notifi-dropdown js-dropdown">
                                            <div class="notifi__title">
                                                <p>Bạn có 3 thông báo</p>
                                            </div>
                                            <div class="notifi__item">
                                                <div class="bg-c1 img-cir img-40">
                                                    <i class="zmdi zmdi-email-open"></i>
                                                </div>
                                                <div class="content">
                                                    <p>
                                                        Bạn đã nhận được một thông báo qua email
                                                    </p>
                                                    <span class="date">23:40, 16/07/2020</span>
                                                </div>
                                            </div>
                                            <div class="notifi__item">
                                                <div class="bg-c2 img-cir img-40">
                                                    <i class="zmdi zmdi-account-box"></i>
                                                </div>
                                                <div class="content">
                                                    <p>
                                                        Tài khoản của bạn đã bị khóa
                                                    </p>
                                                    <span class="date">23:40, 16/07/2020</span>
                                                </div>
                                            </div>
                                            <div class="notifi__item">
                                                <div class="bg-c3 img-cir img-40">
                                                    <i class="zmdi zmdi-file-text"></i>
                                                </div>
                                                <div class="content">
                                                    <p>
                                                        Bạn có một tập tin mới
                                                    </p>
                                                    <span class="date">23:40, 16/07/2020</span>
                                                </div>
                                            </div>
                                            <div class="notifi__footer">
                                                <a href="#">Tất cả thông báo</a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="account-wrap">
                                    <div class="account-item clearfix js-item-menu">
                                        <div class="image">
                                            <img src="~/Areas/Admin/Themes/images/icon/acc.jpg" alt="John Doe" />
                                        </div>
                                        <div class="content">
                                            <a class="js-acc-btn" href="#" style="color:#ff8b00">Trần Thị Phương</a>
                                        </div>
                                        <div class="account-dropdown js-dropdown">
                                            <div class="info clearfix">
                                                <div class="image">
                                                    <a href="#">
                                                        <img src="~/Areas/Admin/Themes/images/icon/acc.jpg" alt="John Doe" />
                                                    </a>
                                                 
                                                </div>
                                            </div>
                                            <div class="account-dropdown__body">
                                                <div class="account-dropdown__item">
                                                    <a href="#">
                                                        <i class="zmdi zmdi-account"></i>Tài khoản
                                                    </a>
                                                </div>
                                                <div class="account-dropdown__item">
                                                    <a href="#">
                                                        <i class="zmdi zmdi-settings"></i>Cài đặt
                                                    </a>
                                                </div>
                                               
                                            </div>
                                            <div class="account-dropdown__footer">
                                                <a href='@Url.Action("Index", "LoginAdmin")'>
                                                    <i class="zmdi zmdi-power"></i>Đăng xuất
                                                </a>
                                               
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </header>
            <!-- HEADER DESKTOP-->
            <!-- MAIN CONTENT-->
            <div style="padding-top : 105px">
                @RenderBody()
            </div>
            
            <!-- END MAIN CONTENT-->
            <!-- END PAGE CONTAINER-->
        </div>
    </div>
    <!-- Jquery JS-->
    <script src="~/Areas/Admin/Themes/vendor/jquery-3.2.1.min.js"></script>
    <!-- Bootstrap JS-->
    <script src="~/Areas/Admin/Themes/vendor/bootstrap-4.1/popper.min.js"></script>
    <script src="~/Areas/Admin/Themes/vendor/bootstrap-4.1/bootstrap.min.js"></script>
    <!-- Vendor JS       -->
    <script src="~/Areas/Admin/Themes/vendor/slick/slick.min.js">
    </script>
    <script src="~/Areas/Admin/Themes/vendor/wow/wow.min.js"></script>
    <script src="~/Areas/Admin/Themes/vendor/animsition/animsition.min.js"></script>
    <script src="~/Areas/Admin/Themes/vendor/bootstrap-progressbar/bootstrap-progressbar.min.js">
    </script>
    <script src="~/Areas/Admin/Themes/vendor/counter-up/jquery.waypoints.min.js"></script>
    <script src="~/Areas/Admin/Themes/vendor/counter-up/jquery.counterup.min.js">
    </script>
    <script src="~/Areas/Admin/Themes/vendor/circle-progress/circle-progress.min.js"></script>
    <script src="~/Areas/Admin/Themes/vendor/perfect-scrollbar/perfect-scrollbar.js"></script>
    <script src="~/Areas/Admin/Themes/vendor/chartjs/Chart.bundle.min.js"></script>
    <script src="~/Areas/Admin/Themes/vendor/select2/select2.min.js">
    </script>
    <!-- Main JS-->
    <script src="~/Areas/Admin/Themes/js/main.js"></script>
</body>
</html>
<!-- end document-->
